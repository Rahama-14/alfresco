               </sequence>
            </complexType>
         </element>
         
         <element name="existsResponse">
            <complexType>
               <sequence>
                  <element name="existsReturn" type="content:ExistsResult" maxOccurs="unbounded" minOccurs="0"/>
               </sequence>
            </complexType>
         </element>
         
         <complexType name="ContentSegment">
            <sequence>
               <element name="offset" type="xsd:long"/>
               <element name="length" type="xsd:long"/>
            </sequence>
         </complexType>
         <element name="ContentSegment" type="content:ContentSegment"/>
         
         <complexType name="ReadResult">
            <sequence>
               <element name="url" type="xsd:string"/>
               <element name="content" type="cms:Content"/>
            </sequence>
         </complexType>
         <element name="ReadResult" type="content:ReadResult"/>
         
         <complexType name="ExistsResult">
            <sequence>
               <element name="content" type="cms:Reference"/>
               <element name="exists" type="xsd:boolean"/>
               <element name="length" type="xsd:long"/>
            </sequence>
         </complexType>
         <element name="ExistsResult" type="content:ExistsResult"/>
         
         <complexType name="ContentFault">
            <sequence>
               <element name="errorCode" type="xsd:int"/>
               <element name="message" type="xsd:string"/>
            </sequence>
         </complexType>
         <element name="ContentFault" type="content:ContentFault"/>
         
            <element name="readChunkContentFault" type="string"></element>
        </schema>
