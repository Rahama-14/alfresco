<model name="test:integrity" xmlns="http://www.alfresco.org/model/dictionary/1.0">

   <description>Test Model for Integrity tests</description>
   <author>Alfresco</author>
   <published>2005-06-05</published>
   <version>0.1</version>

   <imports>
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
      <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
   </imports>
   
   <namespaces>
      <namespace uri="http://www.alfresco.org/test/IntegrityTest" prefix="test"/>
   </namespaces>

   <types>
      <type name="test:folder">
         <title>Content</title>
         <parent>sys:base</parent>
         <properties>
            <property name="test:name">
               <type>d:text</type>
               <mandatory>true</mandatory>
            </property>
            <property name="test:reference">
               <type>d:text</type>
            </property>
         </properties>
         <mandatory-aspects>
            <aspect>test:titled</aspect>
         </mandatory-aspects>
      </type>
   </types>
   
   <aspects>
      <aspect name="test:titled">
         <title>Titled</title>
         <properties>
            <property name="test:title">
               <type>d:text</type>
               <mandatory>true</mandatory>
            </property>
            <property name="test:description">
               <type>d:text</type>
            </property>
         </properties>
      </aspect>
   </aspects>

</model>
