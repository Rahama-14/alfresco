<alfresco-config>
   
   <plug-ins>
      <element-readers>
         <element-reader element-name="property-sheet" class="org.alfresco.web.config.PropertySheetElementReader"/>
         <element-reader element-name="server" class="org.alfresco.web.config.ServerElementReader"/>
      </element-readers>
   </plug-ins>

   <config>
      <property-sheet>
         <show-property name="name" />
      </property-sheet>
      <server>
         <mode>servlet</mode>
         <error-page>/jsp/error.jsp</error-page>
      </server>
   </config>
   
   <config evaluator="string-compare" condition="space-aspect">
      <property-sheet>
         <show-property name="createddate" readOnly="true" />
         <show-property name="modifieddate" readOnly="true" />
         <show-property name="description" displayLabel="Description" readOnly="false" />
         <show-property name="icon" />
      </property-sheet>
   </config>
   
   <config evaluator="object-type" condition="org.alfresco.web.ui.common.component.data.UIRichList">
      <renderers>
         <renderer mode="details">org.alfresco.web.ui.common.renderer.data.RichListRenderer.DetailsViewRenderer</renderer>
      </renderers>
   </config>
   
</alfresco-config>