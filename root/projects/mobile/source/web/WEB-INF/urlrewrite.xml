<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">

   <!-- Spring Surf -->
   <rule>
      <from>/proxy**</from>
      <to>/p/proxy/$1</to>
   </rule>
   <rule>
      <from>/res/**</from>
      <to>/p/resource/$1</to>
   </rule>
   <rule>
      <from>/service/**</from>
      <to>/p/$1</to>
   </rule>

</urlrewrite>