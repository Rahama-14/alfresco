<documentList>
   <actionSets>
      <actionSet id="empty"></actionSet>

      <actionSet id="document">
         <action id="onActionDownload" type="simple-link" href="{downloadUrl}" label="document-actions.download" domId="{htmlid}-download-action" />
         <action id="onActionDetails" type="simple-link" href="{editMetadataUrl}" permission="edit" label="document-actions.details" domId="{htmlid}-edit-metadata-action" />
         <action id="onActionAssignWorkflow" type="action-link" label="document-actions.assign-workflow" />
         <action id="onActionCopyTo" type="action-link" label="document-actions.copy-to" />
         <action id="onActionMoveTo" type="action-link" permission="delete" label="document-actions.move-to" />
         <action id="onActionDelete" type="action-link" permission="delete" label="document-actions.delete" />
         <action id="onActionManagePermissions" type="action-link" permission="permissions" label="document-actions.manage-permissions" />
         <action id="onActionManageAspects" type="action-link" permission="edit" label="document-actions.manage-aspects" />
      </actionSet>

      <actionSet id="locked">
         <action id="onActionDownload" href="{downloadUrl}" type="simple-link" label="document-actions.download" domId="{htmlid}-download-action" />
      </actionSet>

      <actionSet id="lockOwner">
         <action id="onActionDownload" href="{downloadUrl}" type="simple-link" label="document-actions.download" domId="{htmlid}-download-action" />
      </actionSet>

      <actionSet id="workingCopyOwner">
         <action id="onActionDownload" href="{downloadUrl}" type="simple-link" label="document-actions.download" domId="{htmlid}-download-action" />
      </actionSet>

      <actionSet id="link">
         <action id="onActionDelete" permission="delete" type="action-link" label="document-actions.delete" />
      </actionSet>
   </actionSets>
</documentList>
