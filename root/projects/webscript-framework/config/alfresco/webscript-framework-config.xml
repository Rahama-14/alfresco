<alfresco-config>

   <plug-ins>
      <element-readers>
         <element-reader element-name="server" class="org.alfresco.web.config.ServerElementReader"/>
      </element-readers>
   </plug-ins>
   
   <config evaluator="string-compare" condition="Server">
      <server>
         <!-- Enable and adjust the following settings to allow for proxied use of WebScript Framework
              and to return an externally accessable address for absolute url generation
         <scheme>http</scheme>
         <hostname>localhost</hostname>
         <port>8080</port>
         -->
      </server>
   </config>

</alfresco-config>