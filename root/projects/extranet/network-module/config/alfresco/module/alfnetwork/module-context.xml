<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
	
	<!-- Network Bootstraps -->
	<import resource="classpath:alfresco/module/alfnetwork/alf-network-context.xml" /> 
	<import resource="classpath:alfresco/module/alfnetwork/alf-network-ldap-context.xml" />
    
	<!-- Import our model -->
	<bean id="alfnetwork.dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
		<property name="models">
		  	<list>
		    	<value>alfresco/module/alfnetwork/alfNetworkModel.xml</value>        
		    </list>
		</property>
	</bean>

	<!-- Web Client Config -->	
	<bean id="alfnetwork.configBootstrap" class="org.alfresco.web.config.WebClientConfigBootstrap" init-method="init" depends-on="alfnetwork.dictionaryBootstrap">
		<property name="configs">
			<list>
				<value>classpath:alfresco/module/alfnetwork/web-client-custom.xml</value>
			</list>
		</property>
	</bean>  
	
   <!--  Alfresco LDAP Synchronization Web Script -->
   <!--
   	For 2.9 and 3.0
   <bean id="webscript.org.alfresco.network.ldapsync.ldapsync.get" class="org.alfresco.module.alfnetwork.LDAPSyncWebScript" parent="webscript" depends-on="repositoryHelper">
      <property name="repository" ref="repositoryHelper" />
      <property name="serviceRegistry" ref="ServiceRegistry" />
   </bean>
   -->
   <!-- For 2.2 -->
   <bean id="webscript.org.alfresco.network.ldapsync.ldapsync.get" class="org.alfresco.module.alfnetwork.LDAPSyncWebScript" parent="webscript"/>
    
</beans>
