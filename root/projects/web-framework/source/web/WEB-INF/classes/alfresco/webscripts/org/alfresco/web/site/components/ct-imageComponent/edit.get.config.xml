<wizard id="ct-imageComponent">
	<title>Image Component</title>
	<page id="start" start="true" type="form">
		<buttons>
			<button id="cancel" label="Cancel" action="cancel"/>
			<button id="save" label="Save" action="transition" data="submit"/>
		</buttons>
		<data>
			<element id="imageURL" type="String" defaultValue="" />
			<element id="imageWidth" type="String" defaultValue="" />
			<element id="imageHeight" type="String" defaultValue="" />
			<element id="imageAlt" type="String" defaultValue="" />			
		</data>
	</page>
	<page id="submit" end="true" type="form">
		<data>
			<element id="imageURL" type="String" />
			<element id="imageWidth" type="String" />
			<element id="imageHeight" type="String" />
			<element id="imageAlt" type="String" />
		</data>
	</page>	
</wizard>
