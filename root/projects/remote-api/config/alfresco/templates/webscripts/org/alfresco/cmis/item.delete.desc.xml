<webscript>
  <shortname>Delete Object (deleteObject)</shortname>
  <description>
  <![CDATA[
  Delete the specified object.
  ]]>
  </description>
  
  <!-- by object id -->
  <url>/cmis/i/{id}</url>
  <url>/cmis/s/{store}/i/{id}</url>
  <!-- by path -->
  <url>/cmis/p{path}</url>
  <url>/cmis/s/{store}/p{path}</url>
  <!-- alfresco style -->
  <url>/api/node/{store_type}/{store_id}/{id}</url>
  <url>/api/path/{store_type}/{store_id}/{path}</url>

  <format default="atomentry"/>
  
  <authentication>user</authentication>
  <family>CMIS</family>
  <lifecycle>public_api</lifecycle>
</webscript>