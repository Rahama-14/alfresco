<webscript>
  <shortname>Delete the specified assets.</shortname>
  <description>
  <![CDATA[
  Delete the specified assets.
  <CR>
  If the optional webApp argument is specified then returns the modified assets within that web app.
  ]]>
  </description>
  <url>/api/wcm/webprojects/{webprojectref}/sandboxes/{sandboxref}/assets/{path}</url>
  <!-- TEMP COMMENTING OUT - FRAMEWORK BARFS
  <url>/api/wcm/webprojects/{webprojectref}/sandboxes/{sandboxref}/assets/(path}?webApp={webApp?}</url>
  -->
  <format default="json"/>
  <authentication>user</authentication>
  <transaction>required</transaction>
  <family>WCM Service</family>
</webscript>