<webscript>
  <shortname>Retrieve ACL (getACL)</shortname>
  <description>
<![CDATA[
Returns the ACL currently applied to the specified object.<br>
<br>
Inputs:<br>
<br>
Outputs:<br>
<br>
Collection accessControlEntryCollection<br>
<br>
Notes:<br>
<br>
The server may respond using a combination of CMIS Basic permissions and
repository specific permissions.<br>
]]>
  </description>
  <!-- by object id -->
  <url>/cmis/i/{id}/acl</url>
  <url>/cmis/s/{store}/i/{id}/acl</url>
  <!-- by object path -->
  <url>/cmis/p{path}/acl</url>
  <url>/cmis/s/{store}/p{path}/acl</url>
  <!-- alfresco style -->
  <url>/api/node/{store_type}/{store_id}/{id}/acl</url>
  <url>/api/path/{store_type}/{store_id}/{path}/acl</url>
  
  <authentication>guest</authentication>
  <transaction allow="readonly"/>
  <format default="cmisacl">argument</format>
  <family>CMIS</family>
</webscript>