<wizard id="tablelayout">
	<title>Table Layout Region Manager</title>
	<page id="start" start="true" type="form">
		<data>
			<element id="actionFlag" type="String" defaultValue="" />/>			
			<element id="templateId" type="String" defaultValue="" />
			<element id="rowId" type="String" defaultValue="" />
			<element id="panelId" type="String" defaultValue="" />
            <element id="regionName" type="String" defaultValue="" />			
            <element id="regionScope" type="String" defaultValue="" />
            <element id="regionHeight" type="String" defaultValue="" />
		</data>
		<buttons>
			<button id="save" label="Save" action="transition" data="submit"/>
			<button id="cancel" label="Cancel" action="cancel"/>
		</buttons>
	</page>
	<page id="submit" end="true" type="form">
		<data>
            <element id="actionFlag" type="String" />            
            <element id="regionName" type="String" />
            <element id="regionScope" type="String" />
            <element id="templateId" type="String" />                          
            <element id="rowId" type="String" />
            <element id="panelId" type="String" />
            <element id="regionHeight" type="String" />				
		</data>
	</page>	
</wizard>
