<wizard id="navigationadd">
	<title>Add a New Page</title>
	<page id="start" start="true" type="form">
		<buttons>
			<button id="cancel" label="Cancel" action="cancel"/>
			<button id="save" label="Save" action="transition" data="submit"/>
		</buttons>
		<data>
			<element id="parentPageId" type="String" defaultValue="" />
			<element id="pageName" type="String" defaultValue="" />
			<element id="pageDescription" type="String" defaultValue="" />
		</data>
	</page>
	<page id="submit" end="true" type="form">
		<data>
			<element id="parentPageId" type="String" />
			<element id="pageName" type="String" />
			<element id="pageDescription" type="String" />
		</data>
	</page>	
</wizard>
