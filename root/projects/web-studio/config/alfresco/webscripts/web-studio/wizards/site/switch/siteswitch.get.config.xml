<wizard id="siteswitch">
	<title>Select a Web Site</title>
	<page id="start" start="true" type="form">
		<data>
			<element id="webproject" type="String" defaultValue="" />
		</data>
		<buttons>
			<button id="cancel" label="Cancel" action="cancel" />
			<button id="next" label="Select" action="transition" data="exit" />
		</buttons>	
	</page>	
	<page id="exit" end="true" type="form">
		<data>
			<element id="webproject" type="String" />
		</data>
	</page>
</wizard>
