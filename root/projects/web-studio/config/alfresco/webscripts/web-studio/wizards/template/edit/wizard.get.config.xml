<wizard id="welcome">
	<title>Edit Template</title>
	<page id="start" start="true" type="form">
		<buttons>
			<button id="save" label="Save" action="transition" data="submit"/>		
			<button id="cancel" label="Cancel" action="cancel"/>
		</buttons>
		<data>
			<element id="templateId" type="String" defaultValue="" />
			<element id="templateName" type="String" defaultValue="" />
			<element id="templateDescription" type="String" defaultValue="" />
	        <element id="templateLayoutType" type="String" defaultValue="Absolute" />
	        <element id="templateHeight" type="String" defaultValue="" />
	        <element id="templateWidth" type="String" defaultValue="" />	        	        
	        <element id="templateHeight" type="String" defaultValue="" />
	        <element id="templateWidth" type="String" defaultValue="" />	        	        
	        <element id="templateHeight" type="String" defaultValue="" />
	        <element id="templateWidth" type="String" defaultValue="" />	        	        
		</data>
	</page>
	<page id="submit" end="true" type="form">
		<data>
			<element id="templateId" type="String" />
			<element id="templateName" type="String" />
			<element id="templateDescription" type="String" />
            <element id="templateLayoutType" type="String" />			
            <element id="templateHeight" type="String" />
            <element id="templateWidth" type="String" />                        
		</data>
	</page>	
</wizard>
