<wizard id="remove">
	<title>Remove Template</title>
	<page id="start" start="true" type="form">
		<buttons>
			<button id="cancel" label="Cancel" action="cancel"/>
			<button id="remove" label="Remove" action="transition" data="submit"/>
		</buttons>
	</page>
	<page id="submit" end="true" type="form"/>
</wizard>
