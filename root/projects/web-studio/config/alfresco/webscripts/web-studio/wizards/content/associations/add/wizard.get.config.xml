<wizard id="add-content-association">
	<title>Add Content Association</title>
	<page id="start" start="true" type="form">
		<buttons>
			<button id="cancel" label="Cancel" action="cancel"/>
			<button id="save" label="Save" action="transition" data="submit"/>
		</buttons>
		<data>
			<element id="contentId" type="String" defaultValue="" />
			<element id="assocType" type="String" defaultValue="" />
			<element id="formatId" type="String" defaultValue="" />
			<element id="templateId" type="String" defaultValue="" />
		</data>
	</page>
	<page id="submit" end="true" type="form">
		<data>
			<element id="contentId" type="String" />
			<element id="assocType" type="String" />
			<element id="formatId" type="String" />
			<element id="templateId" type="String" />
		</data>
	</page>	
</wizard>
