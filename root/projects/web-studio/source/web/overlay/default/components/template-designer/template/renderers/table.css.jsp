/* TEMPLATE */
.TemplateTable 
{         
	cursor: pointer;	             	
	background-color: #FFFFE0;

	border: 1px dashed black;

	width: 100%;                
    height: 100%;

    vertical-align: top;

	font-family:tahoma,arial,helvetica,sans-serif;
	font-size:11px;
	font-size-adjust:none;
	font-style:normal;
	font-variant:normal;
}    

