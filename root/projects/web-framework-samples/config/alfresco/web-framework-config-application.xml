<alfresco-config>

   <config evaluator="string-compare" condition="WebFramework">
      <web-framework>
      
         <framework-defaults>
            <user-factory>alfresco</user-factory>       
         </framework-defaults>      
         
         <application-defaults>
            <page-type>
            	<id>login</id>
            	<page-instance-id>login</page-instance-id>
            </page-type>
            <page-type>
            	<id>logout</id>
            	<page-instance-id>logout</page-instance-id>
            </page-type>
         </application-defaults>
                  
      </web-framework>
   </config>

</alfresco-config>