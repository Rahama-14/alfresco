<?xml version="1.0" encoding="UTF-8"?>
<testSuite name="ParsingPerformance" xmlns="http://www.sun.com/japex/testSuite">

    <param name="japex.classPath" value="./lib/jdsl/*.jar"/>
    
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/3rd-party/lib/*.jar"/>
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/3rd-party/lib/commons/*.jar"/>  
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/3rd-party/lib/jibx/*.jar"/>  
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/3rd-party/lib/jgroups/*.jar"/>   
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/3rd-party/lib/swarmcache/*.jar"/>   
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/3rd-party/lib/treecache/*.jar"/>    
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/3rd-party/lib/openoffice/*.jar"/>   
    
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/core/build/classes"/> 
    <param name="japex.classPath" value="C:/work/HEAD/root/projects/repository/build/classes"/> 
    <param name="japex.classPath" value="./build/classes"/>  
    <param name="japex.classPath" value="./config"/>   

    <driver name="JAXPDriver">
        <param name="japex.driverClass" value="org.alfresco.benchmark.driver.AlfrescoInternalAPIDriver"/>
        <param name="description" value="Alfresco Internal API"/>
   
    	<param name="japex.numberOfThreads" value="1"/> 
    	<param name="japex.threadRampupTime" value="0"/>
    	<param name="japex.includeWarmupRun" value="false"/>
    	
    </driver>

    <testCase name="Alfresco Internal API">
    
        <!-- Usage Profile -->
        <param name="alfresco.usageProfile.readContent" value="35"/>
        <param name="alfresco.usageProfile.readProperties" value="30"/>
		<param name="alfresco.usageProfile.createContent" value="15"/>		
        <param name="alfresco.usageProfile.createVersion" value="15"/>
        <param name="alfresco.usageProfile.createFolder" value="5"/>
        
        <!-- Repository Profile -->
        <param name="alfrsco.repositoryProfile.folderDepth.average" value="3"/>
        <param name="alfrsco.repositoryProfile.folderDepth.variation" value="1"/>
        <param name="alfrsco.repositoryProfile.documentsInFolderCount.average" value="5"/>
        <param name="alfrsco.repositoryProfile.documentsInFolderCount.variation" value="1"/>
        <param name="alfrsco.repositoryProfile.subFoldersCount.average" value="3"/>
        <param name="alfrsco.repositoryProfile.subFoldersCount.variation" value="1"/>      
        
        <!-- Number of available users -->
        <param name="alfresco.numberOfAvailableUsers" value="50"/>                                       
    
        <param name="japex.warmupIterations" value="0"/>            
    	<param name="japex.runTime" value="00:30"/> 
    	<param name="japex.runDelay" value="0"/>
    	<param name="japex.runDelayVariation" value="0"/>
    </testCase>
    
</testSuite>
