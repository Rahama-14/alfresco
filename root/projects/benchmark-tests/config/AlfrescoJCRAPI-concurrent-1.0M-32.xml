<?xml version="1.0" encoding="UTF-8"?>
<testSuite name="ParsingPerformance" xmlns="http://www.sun.com/japex/testSuite">

    <driver name="JAXPDriver">
        <param name="japex.driverClass" value="org.alfresco.benchmark.alfresco.jcr.AlfrescoJCRDriver"/>
        <param name="description" value="Alfresco Internal API"/>

    	<param name="japex.numberOfThreads" value="32"/>
    	<param name="japex.threadRampupTime" value="60"/>
    	<param name="japex.includeWarmupRun" value="false"/>
    	
    </driver>

    <testCase name="AlfrescoJCRAPI-concurrent-1.0M-32 Read Content">

        <param name="alfresco.usageProfile.readContent" value="100"/>
        <param name="alfresco.usageProfile.readProperties" value="0"/>
		<param name="alfresco.usageProfile.createContent" value="0"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="0"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>                                       

        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    	
    </testCase> 

    <testCase name="AlfrescoJCRAPI-concurrent-1.0M-32 Read Properties">
    
        <param name="alfresco.usageProfile.readContent" value="0"/>
        <param name="alfresco.usageProfile.readProperties" value="100"/>
		<param name="alfresco.usageProfile.createContent" value="0"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="0"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>
    
        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    	
    </testCase>     

    <testCase name="AlfrescoJCRAPI-concurrent-1.0M-32 Create Content">
    
        <!-- Usage Profile -->
        <param name="alfresco.usageProfile.readContent" value="0"/>
        <param name="alfresco.usageProfile.readProperties" value="0"/>
		<param name="alfresco.usageProfile.createContent" value="100"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="0"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>
    
        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    	
    </testCase>

    <testCase name="AlfrescoJCRAPI-concurrent-1.0M-32 Create Folder">
    
        <!-- Usage Profile -->
        <param name="alfresco.usageProfile.readContent" value="0"/>
        <param name="alfresco.usageProfile.readProperties" value="0"/>
		<param name="alfresco.usageProfile.createContent" value="0"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="100"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>
    
        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    	
    </testCase>

    <testCase name="AlfrescoJCRAPI-concurrent-1.0M-32 All Operations (Normal Profile)">
    
        <!-- Usage Profile -->
        <param name="alfresco.usageProfile.readContent" value="80"/>
        <param name="alfresco.usageProfile.readProperties" value="0"/>
		<param name="alfresco.usageProfile.createContent" value="20"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="0"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>
    
        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    </testCase>
    
</testSuite>

