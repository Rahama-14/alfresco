<?xml version="1.0" encoding="UTF-8"?>
<testSuite name="ParsingPerformance" xmlns="http://www.sun.com/japex/testSuite">

    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/core/build/classes"/>       

    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/benchmark-framework/lib/jdsl/*.jar"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/benchmark-framework/lib/*.jar"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/benchmark-framework/build/classes"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/benchmark-framework/config"/>

    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/3rd-party/lib/*.jar"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/3rd-party/lib/commons/*.jar"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/3rd-party/lib/jibx/*.jar"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/3rd-party/lib/jgroups/*.jar"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/3rd-party/lib/swarmcache/*.jar"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/3rd-party/lib/treecache/*.jar"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/3rd-party/lib/openoffice/*.jar"/>

    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/repository/build/classes"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/repository/config"/>

    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/benchmark-tests/build/classes"/>
    <param name="japex.classPath" value="${ALFRESCO_PROJECTS}/benchmark-tests/config"/>

    <driver name="JAXPDriver">
        <param name="japex.driverClass" value="org.alfresco.benchmark.alfresco.jcr.AlfrescoJCRDriver"/>
        <param name="description" value="Alfresco Internal API"/>

    	<param name="japex.numberOfThreads" value="128"/>
    	<param name="japex.threadRampupTime" value="60"/>
    	<param name="japex.includeWarmupRun" value="false"/>
    	
    </driver>

    <testCase name="AlfrescoJCRAPI-concurrent-new-128-Lucene Read Content">

        <param name="alfresco.usageProfile.readContent" value="100"/>
        <param name="alfresco.usageProfile.readProperties" value="0"/>
		<param name="alfresco.usageProfile.createContent" value="0"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="0"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>                                       

        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    	
    </testCase> 

    <testCase name="AlfrescoJCRAPI-concurrent-new-128-Lucene Read Properties">
    
        <param name="alfresco.usageProfile.readContent" value="0"/>
        <param name="alfresco.usageProfile.readProperties" value="100"/>
		<param name="alfresco.usageProfile.createContent" value="0"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="0"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>
    
        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    	
    </testCase>     

    <testCase name="AlfrescoJCRAPI-concurrent-new-128-Lucene Create Content">
    
        <!-- Usage Profile -->
        <param name="alfresco.usageProfile.readContent" value="0"/>
        <param name="alfresco.usageProfile.readProperties" value="0"/>
		<param name="alfresco.usageProfile.createContent" value="100"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="0"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>
    
        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    	
    </testCase>

    <testCase name="AlfrescoJCRAPI-concurrent-new-128-Lucene Create Folder">
    
        <!-- Usage Profile -->
        <param name="alfresco.usageProfile.readContent" value="0"/>
        <param name="alfresco.usageProfile.readProperties" value="0"/>
		<param name="alfresco.usageProfile.createContent" value="0"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="100"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>
    
        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    	
    </testCase>

    <testCase name="AlfrescoJCRAPI-concurrent-new-128-Lucene All Operations (Normal Profile)">
    
        <!-- Usage Profile -->
        <param name="alfresco.usageProfile.readContent" value="80"/>
        <param name="alfresco.usageProfile.readProperties" value="0"/>
		<param name="alfresco.usageProfile.createContent" value="20"/>		
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="0"/>

        <param name="alfresco.numberOfAvailableUsers" value="50"/>
    
        <param name="japex.warmupIterations" value="1"/>            
    	<param name="japex.runIterations" value="10"/>
    	<param name="japex.runDelay" value="15"/>
    	<param name="japex.runDelayVariation" value="10"/>
    </testCase>
    
</testSuite>

