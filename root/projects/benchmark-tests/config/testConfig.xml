<?xml version="1.0" encoding="UTF-8"?>
<testSuite name="ParsingPerformance" xmlns="http://www.sun.com/japex/testSuite">
    
    <driver name="JAXPDriver">
        <param name="japex.driverClass" value="org.alfresco.benchmark.alfresco.jcr.AlfrescoJCRBulkLoadDriver"/>
        <param name="description" value="Alfresco Internal API"/>   
    	<param name="japex.numberOfThreads" value="1"/> 
    	<param name="japex.threadRampupTime" value="0"/>
    	<param name="japex.includeWarmupRun" value="false"/>

    </driver>

    <testCase name="testConfig">

        <!-- Usage Profile -->
        <param name="alfresco.usageProfile.readContent" value="25"/>
        <param name="alfresco.usageProfile.readProperties" value="25"/>
        <param name="alfresco.usageProfile.createContent" value="25"/>
        <param name="alfresco.usageProfile.createVersion" value="0"/>
        <param name="alfresco.usageProfile.createFolder" value="25"/>
        
        <!-- Number of available users -->
        <param name="alfresco.numberOfAvailableUsers" value="50"/>                                       

        <param name="japex.warmupIterations" value="0"/>
 <!--   	<param name="japex.runTime" value="01:00"/> -->
    	<param name="japex.runIterations" value="5"/>
    	
    	<param name="alfresco.fileCount" value="10"/>
    	<param name="alfresco.folderCount" value="2"/>

    	<param name="japex.runDelay" value="0"/>
    	<param name="japex.runDelayVariation" value="0"/>

    </testCase>

</testSuite>
