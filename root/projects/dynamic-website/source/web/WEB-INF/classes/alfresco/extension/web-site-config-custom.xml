<alfresco-config>

	<config evaluator="string-compare" condition="WebFramework">
	
		<!-- Include the ADW Tag Library -->
		<tags>
			<libraries>
				<library>
					<id>adw</id>
					<uri>/WEB-INF/tlds/adw.tld</uri>
					<namespace>http://www.alfresco.org/taglib/adw</namespace>				
				</library>				
			</libraries>
		</tags>
		
		<!-- Change the welcome page to our getting started one -->
		<page>
			<default>
				<uri>/ui/misc/getting-started.jsp</uri>
			</default>
		</page>
		
		
	</config>

</alfresco-config>
