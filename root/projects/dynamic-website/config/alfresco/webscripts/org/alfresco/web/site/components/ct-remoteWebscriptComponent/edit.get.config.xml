<wizard id="ct-remoteWebscriptComponent">
	<title>Remote Webscript Component</title>
	<page id="start" start="true" type="form">
		<buttons>
			<button id="cancel" label="Cancel" action="cancel"/>
			<button id="save" label="Save" action="transition" data="submit"/>
		</buttons>
		<data>
			<element id="endpointId" type="String" defaultValue="" />
			<element id="webscript" type="String" defaultValue="" />
			<element id="container" type="String" defaultValue="" />
		</data>
	</page>
	<page id="submit" end="true" type="form">
		<data>
			<element id="endpointId" type="String" />
			<element id="webscript" type="String" />
			<element id="container" type="String" />
		</data>
	</page>	
</wizard>
