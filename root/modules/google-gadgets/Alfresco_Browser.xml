<?xml version="1.0" encoding="UTF-8"?>
<Module>

<ModulePrefs
   title="Alfresco Browser"
   description="Alfresco Document and Folder Browser"
   author="Alfresco Software"
   scrolling="true"
   height="350">
</ModulePrefs>

<UserPref
   name="path"
   display_name="Starting Folder"
   required="false" />

