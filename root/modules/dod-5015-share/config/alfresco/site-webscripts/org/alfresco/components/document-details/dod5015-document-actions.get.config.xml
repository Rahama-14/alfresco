<documentList>
   <actionSets>
      <actionSet id="empty"></actionSet>

      <actionSet id="record">
         <action id="onActionDownload" permission="" type="simple-link" href="{downloadUrl}" label="actions.download" />
         <action id="onActionEditDetails" permission="edit" type="simple-link" href="{editMetadataUrl}" label="actions.edit-details" />
         <action id="onActionReviewed" permission="reviewed" type="action-link" label="actions.reviewed" />
         <action id="onActionCutoff" permission="cutoff" type="action-link" label="actions.cutoff" />
         <action id="onActionRetain" permission="retain" type="action-link" label="actions.retain" />
         <action id="onActionDestroy" permission="destroy" type="action-link" label="actions.destroy" />
         <action id="onActionTransfer" permission="transfer" type="action-link" label="actions.transfer" />
         <action id="onActionFreeze" permission="freeze" type="action-link" label="actions.freeze" />
         <action id="onActionUnfreeze" permission="unfreeze" type="action-link" label="actions.unfreeze" />
         <action id="onActionUndeclare" permission="UndeclareRecords" type="action-link" label="actions.undeclare" />
         <action id="onActionCopyTo" permission="" type="action-link" label="actions.copy-to" />
         <action id="onActionMoveTo" permission="delete" type="action-link" label="actions.move-to" />
         <action id="onActionFileTo" permission="" type="action-link" label="actions.file-to" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.delete" />
      </actionSet>

      <actionSet id="undeclaredRecord">
         <action id="onActionDownload" permission="" type="simple-link" href="{downloadUrl}" label="actions.download" />
         <action id="onActionEditDetails" permission="edit" type="simple-link" href="{editMetadataUrl}" label="actions.edit-details" />
         <action id="onActionDeclare" permission="DeclareRecords" type="action-link" label="actions.declare" />
         <action id="onActionUploadNewVersion" permission="edit" type="action-link" label="actions.upload-new-version" />
         <action id="onActionCopyTo" permission="" type="action-link" label="actions.copy-to" />
         <action id="onActionMoveTo" permission="delete" type="action-link" label="actions.move-to" />
         <action id="onActionFileTo" permission="" type="action-link" label="actions.file-to" />
         <action id="onActionSplitEmail" permission="edit" type="action-link" label="actions.split-email" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.delete" />
      </actionSet>

      <actionSet id="nonElectronicRecord">
      </actionSet>
   </actionSets>
</documentList>