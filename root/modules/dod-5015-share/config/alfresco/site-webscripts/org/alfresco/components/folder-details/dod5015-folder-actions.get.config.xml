<documentList>
   <actionSets>
      <actionSet id="empty"></actionSet>

      <actionSet id="recordSeries">
         <action type="simple-link" id="onActionEditDetails" permission="edit" href="{editMetadataUrl}" label="actions.edit-details" />
         <action type="action-link" id="onActionDelete" permission="HIDDEN" label="actions.delete" />
      </actionSet>

      <actionSet id="recordCategory">
         <action type="simple-link" id="onActionEditDetails" permission="edit" href="{editMetadataUrl}" label="actions.edit-details" />
         <action type="action-link" id="onActionDelete" permission="HIDDEN" label="actions.delete" />
      </actionSet>

      <actionSet id="recordFolder">
         <action type="simple-link" id="onActionEditDetails" permisson="edit" href="{editMetadataUrl}" label="actions.edit-details" />
         <action type="action-link" id="onActionCloseFolder" permission="close-folder" label="actions.close-folder" />
         <action type="action-link" id="onActionOpenFolder" permission="open-folder" label="actions.open-folder" />
         <action type="action-link" id="onActionCutoff" permission="cutoff" label="actions.cutoff" />
         <action type="action-link" id="onActionRetain" permission="hidden" label="actions.retain" />
         <action type="action-link" id="onActionDestroy" permission="destroy" label="actions.destroy" />
         <action type="action-link" id="onActionTransfer" permission="transfer" label="actions.transfer" />
         <action type="action-link" id="onActionFreeze" permission="ExtendRetentionPeriodOrFreeze" label="actions.freeze" />
         <action type="action-link" id="onActionUnfreeze" permission="Unfreeze" label="actions.unfreeze" />
         <action type="action-link" id="onActionCopyTo" permission="" label="actions.copy-to" />
         <action type="action-link" id="onActionMoveTo" permission="delete" label="actions.move-to" />
         <action type="action-link" id="onActionFileTo" permission="" label="actions.file-to" />
         <action type="action-link" id="onActionDelete" permission="HIDDEN" label="actions.delete" />
      </actionSet>

      <actionSet id="folder">
         <action type="simple-link" id="onActionDetails" permission="edit" href="{editMetadataUrl}" label="actions.edit-details" />
         <action type="action-link" id="onActionCopyTo" label="actions.copy-to" />
         <action type="action-link" id="onActionMoveTo" permission="delete" label="actions.move-to" />
         <action type="action-link" id="onActionDelete" permission="HIDDEN" label="actions.delete" />
      </actionSet>
   </actionSets>
</documentList>