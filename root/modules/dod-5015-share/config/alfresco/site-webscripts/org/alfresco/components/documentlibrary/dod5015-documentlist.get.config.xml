<documentList>
   <actionSets>
      <actionSet id="empty"></actionSet>

      <actionSet id="recordSeries">
         <action id="onActionMetadata" type="simple-link" href="{folderDetailsUrl}" label="actions.series.metadata" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.series.delete" />
      </actionSet>

      <actionSet id="recordCategory">
         <action id="onActionMetadata" type="simple-link" href="{folderDetailsUrl}" label="actions.category.metadata" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.category.delete" />
      </actionSet>

      <actionSet id="recordFolder">
         <action id="onActionCloseFolder" permission="close" type="action-link" label="actions.folder.close" />
         <action id="onActionReopenFolder" permission="reopen" type="action-link" label="actions.folder.reopen" />
         <action id="onActionCutoffFolder" permission="cutoff" type="action-link" label="actions.folder.cutoff" />
         <action id="onActionUncutoffFolder" permission="uncutoff" type="action-link" label="actions.folder.uncutoff" />
         <action id="onActionMetadata" type="simple-link" href="{folderDetailsUrl}" label="actions.folder.metadata" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.folder.delete" />
      </actionSet>

      <actionSet id="record">
         <action id="onActionReviewedRecord" permission="reviewed" type="action-link" label="actions.record.reviewed" />
         <action id="onActionCutoffRecord" permission="cutoff" type="action-link" label="actions.record.cutoff" />
         <action id="onActionTransferRecord" permission="transfer" type="action-link" label="actions.record.transfer" />
         <action id="onActionAddReference" permission="add-reference" type="action-link" label="actions.record.add-reference" />
         <action id="onActionFreezeRecord" permission="freeze" type="action-link" label="actions.record.freeze" />
         <action id="onActionUnfreezeRecord" permission="unfreeze" type="action-link" label="actions.record.unfreeze" />
         <action id="onActionDestroyRecord" permission="destroy" type="action-link" label="actions.record.destroy" />
      </actionSet>

      <actionSet id="undeclaredRecord">
         <action id="onActionDeclareRecord" permission="" type="action-link" label="actions.undeclared.declare" />
         <action id="onActionUploadNewVersion" permission="create,edit" type="action-link" label="actions.undeclared.upload-new-version" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.undeclared.delete" />
      </actionSet>

      <actionSet id="nonElectronicRecord">
      </actionSet>

      <actionSet id="link">
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.link.delete" />
      </actionSet>
   </actionSets>
</documentList>