<documentList>
   <actionSets>
      <actionSet id="empty"></actionSet>

      <actionSet id="recordSeries">
         <action id="onActionViewDetails" type="simple-link" href="{recordSeriesDetailsUrl}" label="actions.view-details" />
         <action id="onActionEditDetails" permisson="edit" type="simple-link" href="{editMetadataUrl}" label="actions.edit-details" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.delete" />
      </actionSet>

      <actionSet id="recordCategory">
         <action id="onActionViewDetails" type="simple-link" href="{recordCategoryDetailsUrl}" label="actions.view-details" />
         <action id="onActionEditDetails" permisson="edit" type="simple-link" href="{editMetadataUrl}" label="actions.edit-details" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.delete" />
      </actionSet>

      <actionSet id="recordFolder">
         <action id="onActionViewDetails" type="simple-link" href="{recordFolderDetailsUrl}" label="actions.view-details" />
         <action id="onActionEditDetails" permisson="edit" type="simple-link" href="{editMetadataUrl}" label="actions.edit-details" />
         <action id="onActionClose" permission="close" type="action-link" label="actions.close" />
         <action id="onActionReopen" permission="reopen" type="action-link" label="actions.reopen" />
         <action id="onActionCutoff" permission="cutoff" type="action-link" label="actions.cutoff" />
         <action id="onActionUncutoff" permission="uncutoff" type="action-link" label="actions.uncutoff" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.delete" />
      </actionSet>

      <actionSet id="record">
         <action id="onActionEditDetails" permisson="edit" type="simple-link" href="{editMetadataUrl}" label="actions.edit-details" />
         <action id="onActionReviewed" permission="reviewed" type="action-link" label="actions.reviewed" />
         <action id="onActionCutoff" permission="cutoff" type="action-link" label="actions.cutoff" />
         <action id="onActionTransfer" permission="transfer" type="action-link" label="actions.transfer" />
         <action id="onActionTransferNARA" permission="transferNARA" type="action-link" label="actions.transferNARA" />
         <action id="onActionAddReference" permission="add-reference" type="action-link" label="actions.add-reference" />
         <action id="onActionFreeze" permission="freeze" type="action-link" label="actions.freeze" />
         <action id="onActionUnfreeze" permission="unfreeze" type="action-link" label="actions.unfreeze" />
         <action id="onActionDestroy" permission="destroy" type="action-link" label="actions.destroy" />
         <action id="onActionCopyTo" type="action-link" permission="" label="actions.copy-to" />
         <action id="onActionMoveTo" type="action-link" permission="delete" label="actions.move-to" />
         <action id="onActionFileTo" type="action-link" permission="" label="actions.file-to" />
      </actionSet>

      <actionSet id="undeclaredRecord">
         <action id="onActionEditDetails" permisson="edit" type="simple-link" href="{editMetadataUrl}" label="actions.edit-details" />
         <action id="onActionDeclare" permission="" type="action-link" label="actions.declare" />
         <action id="onActionUploadNewVersion" permission="HIDDEN" type="action-link" label="actions.upload-new-version" />
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.delete" />
         <action id="onActionCopyTo" type="action-link" permission="" label="actions.copy-to" />
         <action id="onActionMoveTo" type="action-link" permission="delete" label="actions.move-to" />
         <action id="onActionFileTo" type="action-link" permission="" label="actions.file-to" />
      </actionSet>

      <actionSet id="nonElectronicRecord">
      </actionSet>

      <actionSet id="folder">
         <action id="onActionViewDetails" type="simple-link" href="{folderDetailsUrl}" label="actions.view-details" />
         <action id="onActionDelete" permission="delete" type="action-link" label="actions.delete" />
      </actionSet>

      <actionSet id="link">
         <action id="onActionDelete" permission="HIDDEN" type="action-link" label="actions.link.delete" />
      </actionSet>
   </actionSets>
</documentList>