<alfresco-config>
   
   	<!-- Add the mediaWiki Space to the create space wizard -->
   	<config evaluator="string-compare" condition="Space Wizards">
      	<folder-types>
         	<type name="mw:mediaWiki" description="MediaWiki" />
      	</folder-types>
   	</config>
   	
   	<config evaluator="node-type" condition="mw:mediaWikiConfig">
   		<property-sheet>
   			<show-property name="mwcp:wgSitename"/>
   			<show-property name="mwcp:wgLogo"/>   			
   		</property-sheet>   	
   	</config>
      
</alfresco-config>
