<?xml version="1.0" encoding="utf-8"?>
<home:HomePanelTopClass 
	xmlns:mx="http://www.adobe.com/2006/mxml" 
	xmlns:home="app.home.*"
	xmlns:local="*"
	height="31" 
	styleName="homePanelTop">

	<mx:Style source="home.css"/>
	
	<mx:HBox width="100%" height="100%" verticalAlign="middle">
		
		<mx:Spacer width="15"/>
		<mx:Label id="labelTitle" styleName="homePanelTitle"/>
		
		<mx:HBox width="100%" height="100%" horizontalAlign="right" verticalAlign="middle">
		
			<mx:Label id="minMaxButton" styleName="homePanelTitle" text="^^"/>	
			<mx:Spacer width="2"/>
			<mx:VRule height="15" width="1" styleName="homeSeparator"/>
			<mx:Spacer width="2"/>
			<mx:Label id="closeButton" styleName="homePanelTitle" text="X"/> 
			<!-- <local:close id="closeButton"/> -->
								
		</mx:HBox>
	</mx:HBox>
	
	
</home:HomePanelTopClass>
