<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" height="25" styleName="homePanelTop">

	<mx:Style source="home.css"/>
	
	<mx:Script>
		<![CDATA[
			import mx.controls.Text;
			
			private var _title:String;
			
			public function set title(value:String):void
			{
				this._title = value;
			}
			
		]]>
	</mx:Script>
	
	<mx:HBox width="100%" height="100%" verticalAlign="middle">
		
		<mx:Spacer width="15"/>
		<mx:Label id="labelTitle" styleName="homePanelTitle" text="{_title}"/>
		
	</mx:HBox>
	
	
</mx:Canvas>
