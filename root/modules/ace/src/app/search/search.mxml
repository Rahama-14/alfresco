<?xml version="1.0" encoding="utf-8"?>
<search:SearchClass xmlns:mx="http://www.adobe.com/2006/mxml" 
		   			xmlns:search="app.search.*"
		   			xmlns:sb="*" 
					styleName="search">	

	<!-- Import css style sheet -->
    <mx:Style source="search.css"/> 
   
   		
	<mx:HBox width="100%" height="100%"  horizontalAlign="left" verticalAlign="middle" horizontalGap="10">
		
		<!--<mx:Spacer width="25"/>	 -->
		<mx:Image source="@Embed(source='../../images/ace-mark.png')" />	
		<!--<mx:Spacer width="30"/>	 -->
		<mx:TextInput id="searchTxt" width="250"  enter="onSearchButtonLinkClick(searchTxt.text)" />
		<!--<mx:Spacer width="15"/> -->
		<mx:Button label="Search" click="onSearchButtonLinkClick(searchTxt.text)"/>
		<!--<mx:Spacer width="25"/> -->
		<mx:Label text="Advanced Search" textDecoration="underline" color="#FFFFFF"/>
					
	</mx:HBox>
	
</search:SearchClass>			