<?xml version='1.0' encoding='UTF-8'?>

<!-- Default Audit Configuration -->

<Audit
    xmlns="http://www.alfresco.org/repo/audit/model/3.2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.alfresco.org/repo/audit/model/3.2 alfresco-audit-3.2.xsd"
    >

    <DataExtractors>
       <DataExtractor name="simpleValue" class="org.alfresco.repo.audit.extractor.SimpleValueDataExtractor"/>
    </DataExtractors>

    <Application name="DOD5015" key="DOD5015">
        <AuditPath key="actions">
            <AuditPath key="testAction">
                <AuditPath key="node">
                    <RecordValue key="noderef" dataExtractor="simpleValue"/>
                </AuditPath>
                <AuditPath key="parameters">
                    <AuditPath key="testActionParam">
                        <RecordValue key="value" dataExtractor="simpleValue"/>
                    </AuditPath>
                </AuditPath>
            </AuditPath>
        </AuditPath>
    </Application>

</Audit>
